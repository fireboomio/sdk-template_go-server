package generated

import (
    "custom-go/pkg/plugins"
    "custom-go/pkg/types"
)

var (
{{#each operations}}
    {{#if (isAllTrue (invertBool isSubscription) (equalAny engine '0,1'))}}
    {{upperFirst name}} = plugins.NewOperationMeta[{{upperFirst name}}{{#equal engine 0}}Internal{{/equal}}Input, {{upperFirst name}}ResponseData]("{{path}}", {{type}})
    {{/if~}}
{{/each}}
)

type (
{{#each operations}}
    {{#if (isAllTrue (invertBool isSubscription) (equalAny engine '0,1'))}}
    {{upperFirst name}}Body = *types.OperationBody[{{upperFirst name}}{{#equal engine 0}}Internal{{/equal}}Input, {{upperFirst name}}ResponseData]
    {{/if~}}
{{/each}}
)

var (
{{#each s3Providers}}
    UploadClient_{{name}} = plugins.NewUploadClient("{{name}}")
{{/each}}
)

const (
{{#each s3Providers}}
    {{#each uploadProfiles as |profile key|}}
    UploadClient_{{name}}_{{key}} plugins.UploadProfile = "{{key}}"
    {{/each}}
{{/each}}
)