package generated

{{#if operations}}
import "custom-go/pkg/plugins"
import "custom-go/pkg/base"
{{/if}}

var (
{{#each operations}}
    {{#if (isAllTrue (invertBool isSubscription) (equalAny engine '0,1'))}}
    {{upperFirst name}} = &plugins.Meta[{{upperFirst name}}{{#equal engine 0}}Internal{{/equal}}Input, {{upperFirst name}}ResponseData]{"{{path}}", {{type}} }
    {{/if~}}
{{/each}}
)

type (
{{#each operations}}
    {{#if (isAllTrue (invertBool isSubscription) (equalAny engine '0,1'))}}
    {{upperFirst name}}Body = *base.OperationBody[{{upperFirst name}}{{#equal engine 0}}Internal{{/equal}}Input, {{upperFirst name}}ResponseData]
    {{/if~}}
{{/each}}
)

var (
{{#each s3Providers}}
    UploadClient_{{name}} = &plugins.UploadClient{"{{name}}"}
{{/each}}
)

const (
{{#each s3Providers}}
    {{#each uploadProfiles as |profile key|}}
    UploadClient_{{name}}_{{key}} plugins.UploadProfile = "{{key}}"
    {{/each}}
{{/each}}
)