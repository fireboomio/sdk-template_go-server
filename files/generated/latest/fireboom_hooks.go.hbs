package latest

{{#if operations}}
import "custom-go/pkg/plugins"
import "custom-go/generated"
{{/if}}

var (
{{#each operations}}
    {{#if (isAllTrue (invertBool isSubscription) (equalAny engine '0,1'))}}
    {{upperFirst name}} = plugins.NewOperationMeta[generated.{{upperFirst name}}{{#equal engine 0}}Internal{{/equal}}Input, generated.{{upperFirst name}}ResponseData]("{{path}}", {{type}})
    {{/if~}}
{{/each}}
)

var (
{{#each s3Providers}}
    UploadClient_{{name}} = &plugins.UploadClient{"{{name}}"}
{{/each}}
)

const (
{{#each s3Providers}}
    {{#each uploadProfiles as |profile key|}}
    UploadClient_{{name}}_{{key}} plugins.UploadProfile = "{{key}}"
    {{/each}}
{{/each}}
)
